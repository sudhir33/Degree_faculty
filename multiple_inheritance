class spider:
    def __init__(self):
        self.power=2
    def display1(self):
        print(self.power,"spider")

class super_man:
    def __init__(self):
        self.power=2
    def display(self):
        print(self.power,"super")


class super_spider(spider,super_man):
    def display(self):
        super().display()
        print("Super_spider")
        
s1=super_spider()
s1.display()
